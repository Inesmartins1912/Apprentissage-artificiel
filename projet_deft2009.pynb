    "# ============================================================\n",
    "# 2. VALIDATION STRUCTURE XML (NOUVEAU)\n",
    "# ============================================================\n",
    "\n",
    "def valider_structure_xml(chemin):\n",
    "    \"\"\"\n",
    "    Valide la structure XML AVANT chargement complet.\n",
    "\n",
    "    Returns:\n",
    "        tuple: (valide: bool, rapport: dict)\n",
    "    \"\"\"\n",
    "    rapport = {\n",
    "        'fichier': Path(chemin).name,\n",
    "        'valide': False,\n",
    "        'docs_total': 0,\n",
    "        'docs_sans_parti': 0,\n",
    "        'docs_sans_texte': 0,\n",
    "        'erreurs': []\n",
    "    }\n",
    "\n",
    "    # Vérifier existence fichier\n",
    "    if not Path(chemin).exists():\n",
    "        rapport['erreurs'].append(f\"Fichier introuvable: {chemin}\")\n",
    "        return False, rapport\n",
    "\n",
    "    # Parser XML\n",
    "    try:\n",
    "        arbre = ET.parse(chemin)\n",
    "        racine = arbre.getroot()\n",
    "    except ET.ParseError as e:\n",
    "        rapport['erreurs'].append(f\"XML malformé: {e}\")\n",
    "        return False, rapport\n",
    "    except Exception as e:\n",
    "        rapport['erreurs'].append(f\"Erreur inattendue: {e}\")\n",
    "        return False, rapport\n",
    "\n",
    "    # Vérifier structure\n",
    "    docs = racine.findall('doc')\n",
    "    rapport['docs_total'] = len(docs)\n",
    "\n",
    "    if len(docs) == 0:\n",
    "        rapport['erreurs'].append(\"Aucun élément <doc> trouvé\")\n",
    "        return False, rapport\n",
    "\n",
    "    # Échantillonner premiers 100 docs\n",
    "    for i, doc in enumerate(docs[:100]):\n",
    "        if doc.find('.//PARTI') is None:\n",
    "            rapport['docs_sans_parti'] += 1\n",
    "            if i < 5:\n",
    "                rapport['erreurs'].append(f\"Doc {i}: balise PARTI manquante\")\n",
    "\n",
    "        elem_texte = doc.find('texte')\n",
    "        if elem_texte is None or not ''.join([p.text or '' for p in elem_texte.findall('p')]).strip():\n",
    "            rapport['docs_sans_texte'] += 1\n",
    "            if i < 5:\n",
    "                rapport['erreurs'].append(f\"Doc {i}: balise texte manquante/vide\")\n",
    "\n",
    "    # Extrapoler sur corpus complet\n",
    "    if len(docs) > 100:\n",
    "        rapport['docs_sans_parti'] = int(rapport['docs_sans_parti'] * len(docs) / 100)\n",
    "        rapport['docs_sans_texte'] = int(rapport['docs_sans_texte'] * len(docs) / 100)\n",
    "\n",
    "    rapport['valide'] = True\n",
    "    return True, rapport\n",
    "\n",
    "\n",
    "def afficher_rapport_validation(rapport):\n",
    "    \"\"\"Affiche un rapport de validation formaté.\"\"\"\n",
    "    print(\"\\n\" + \"=\"*80)\n",
    "    print(f\"VALIDATION : {rapport['fichier']}\")\n",
    "    print(\"=\"*80)\n",
    "\n",
    "    if not rapport['valide']:\n",
    "        print(\" VALIDATION ÉCHOUÉE\\n\")\n",
    "        for erreur in rapport['erreurs']:\n",
    "            print(f\"  • {erreur}\")\n",
    "        return\n",
    "\n",
    "    print(f\" Fichier valide : {rapport['docs_total']:,} documents trouvés\\n\")\n",
    "\n",
    "    if rapport['docs_sans_parti'] > 0:\n",
    "        pct = rapport['docs_sans_parti'] / rapport['docs_total'] * 100\n",
    "        print(f\"  Documents sans parti : {rapport['docs_sans_parti']:,} ({pct:.1f}%)\")\n",
    "\n",
    "    if rapport['docs_sans_texte'] > 0:\n",
    "        pct = rapport['docs_sans_texte'] / rapport['docs_total'] * 100\n",
    "        print(f\"  Documents sans texte : {rapport['docs_sans_texte']:,} ({pct:.1f}%)\")\n",
    "\n",
    "    if len(rapport['erreurs']) > 0:\n",
    "        print(f\"\\n  Avertissements ({len(rapport['erreurs'])}) :\")\n",
    "        for erreur in rapport['erreurs'][:10]:\n",
    "            print(f\"  • {erreur}\")\n",
    "\n",
    "    docs_valides = rapport['docs_total'] - rapport['docs_sans_parti'] - rapport['docs_sans_texte']\n",
    "    pct_valides = docs_valides / rapport['docs_total'] * 100\n",
    "    print(f\"\\n Documents exploitables : ~{docs_valides:,} ({pct_valides:.1f}%)\")\n",
    "\n",
    "\n",
    "print(\"[OK] Fonctions de validation définies\")"
   ]
