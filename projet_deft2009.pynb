   "source": [
    "# ============================================================\n",
    "# 6. PRÉTRAITEMENT LINGUISTIQUE\n",
    "# ============================================================\n",
    "\n",
    "print(\"=\"*80)\n",
    "print(\"PRÉTRAITEMENT LINGUISTIQUE\")\n",
    "print(\"=\"*80)\n",
    "\n",
    "# Chargement spaCy\n",
    "try:\n",
    "    nlp = spacy.load(\"fr_core_news_sm\", disable=[\"parser\", \"ner\"])\n",
    "    print(\"[OK] Modèle spaCy chargé : fr_core_news_sm\")\n",
    "except OSError:\n",
    "    print(\"[ERREUR] Modèle spaCy non installé\")\n",
    "    print(\"Exécutez : python -m spacy download fr_core_news_sm\")\n",
    "    raise\n",
    "\n",
    "# Stopwords étendus\n",
    "STOP_WORDS_FR = {\n",
    "    'au', 'aux', 'avec', 'ce', 'ces', 'dans', 'de', 'des', 'du', 'elle', 'en',\n",
    "    'et', 'eux', 'il', 'je', 'la', 'le', 'les', 'leur', 'lui', 'ma', 'mais',\n",
    "    'me', 'même', 'mes', 'moi', 'mon', 'ne', 'nos', 'notre', 'nous', 'on', 'ou',\n",
    "    'par', 'pas', 'pour', 'qu', 'que', 'qui', 'sa', 'se', 'ses', 'son', 'sur',\n",
    "    'ta', 'te', 'tes', 'toi', 'ton', 'tu', 'un', 'une', 'vos', 'votre', 'vous',\n",
    "    'été', 'étée', 'étées', 'étés', 'étant', 'être', 'suis', 'es', 'est',\n",
    "    'sommes', 'êtes', 'sont', 'ai', 'as', 'avons', 'avez', 'ont', 'avoir'\n",
    "}\n",
    "\n",
    "def pretraiter_texte(texte):\n",
    "    \"\"\"Lemmatise et nettoie un texte français.\"\"\"\n",
    "    doc = nlp(texte.lower())\n",
    "    tokens = [\n",
    "        token.lemma_\n",
    "        for token in doc\n",
    "        if token.is_alpha\n",
    "        and len(token) > 2\n",
    "        and not token.is_stop\n",
    "        and token.lemma_ not in STOP_WORDS_FR\n",
    "    ]\n",
    "    return ' '.join(tokens)\n",
    "\n",
    "\n",
    "# Application\n",
    "print(f\"\\n[INFO] Lemmatisation en cours sur {len(textes_train):,} textes...\")\n",
    "print(\"        (cela prend 2-3 minutes)\")\n",
    "\n",
    "debut = time.time()\n",
    "X_train_clean = []\n",
    "\n",
    "for i, texte in enumerate(textes_train):\n",
    "    X_train_clean.append(pretraiter_texte(texte))\n",
    "\n",
    "    if (i + 1) % 5000 == 0:\n",
    "        print(f\"  → {i+1:,}/{len(textes_train):,} textes traités...\")\n",
    "\n",
    "duree = time.time() - debut\n",
    "\n",
    "print(f\"\\n[OK] Prétraitement terminé en {duree:.1f}s\")\n",
    "print(f\"  Vitesse : {len(textes_train)/duree:.0f} textes/seconde\")\n",
    "\n",
    "# Exemple\n",
    "print(f\"\\n[EXEMPLE TRANSFORMATION]\")\n",
    "print(f\"AVANT : {textes_train[0][:150]}...\")\n",
    "print(f\"APRÈS : {X_train_clean[0][:150]}...\")"
