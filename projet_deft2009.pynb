    "# ============================================================\n",
    "# 5. ANALYSE EXPLORATOIRE\n",
    "# ============================================================\n",
    "\n",
    "# Préparation\n",
    "textes_train = [i['texte'] for i in interventions_train]\n",
    "partis_train = [i['parti'] for i in interventions_train]\n",
    "\n",
    "print(\"\\n\" + \"=\"*80)\n",
    "print(\"ANALYSE EXPLORATOIRE - TRAIN\")\n",
    "print(\"=\"*80)\n",
    "\n",
    "# Distribution classes\n",
    "compteur_partis = Counter(partis_train)\n",
    "df_partis = pd.DataFrame(compteur_partis.items(), columns=['Parti', 'Nombre'])\n",
    "df_partis['Pourcentage'] = (df_partis['Nombre'] / df_partis['Nombre'].sum() * 100).round(2)\n",
    "df_partis = df_partis.sort_values('Nombre', ascending=False)\n",
    "\n",
    "print(\"\\nDistribution des partis :\")\n",
    "print(df_partis.to_string(index=False))\n",
    "\n",
    "# Déséquilibre\n",
    "ratio = df_partis['Nombre'].max() / df_partis['Nombre'].min()\n",
    "print(f\"\\nRatio max/min : {ratio:.2f}\")\n",
    "\n",
    "if ratio > 3:\n",
    "    print(\"  Déséquilibre important détecté\")\n",
    "    print(\"  Stratégie : class_weight='balanced' + StratifiedKFold\")\n",
    "\n",
