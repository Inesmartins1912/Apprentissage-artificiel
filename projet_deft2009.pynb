    "# ============================================================\n",
    "# 4. CHARGEMENT DONNÉES (AMÉLIORÉ)\n",
    "# ============================================================\n",
    "\n",
    "def charger_xml(chemin, verbose=True):\n",
    "    \"\"\"\n",
    "    Charge interventions depuis XML DEFT 2009 avec gestion erreurs robuste.\n",
    "\n",
    "    Args:\n",
    "        chemin: Chemin vers fichier XML\n",
    "        verbose: Afficher statistiques\n",
    "\n",
    "    Returns:\n",
    "        list: Liste de dicts {'parti': str, 'texte': str}\n",
    "    \"\"\"\n",
    "    debut = time.time()\n",
    "\n",
    "    # Parsing avec gestion erreurs\n",
    "    try:\n",
    "        arbre = ET.parse(chemin)\n",
    "        racine = arbre.getroot()\n",
    "    except ET.ParseError as e:\n",
    "        raise ValueError(f\"XML malformé dans {chemin}: {e}\")\n",
    "    except FileNotFoundError:\n",
    "        raise FileNotFoundError(f\"Fichier introuvable: {chemin}\")\n",
    "    except Exception as e:\n",
    "        raise RuntimeError(f\"Erreur inattendue lors du parsing: {e}\")\n",
    "\n",
    "    interventions = []\n",
    "\n",
    "    # Statistiques\n",
    "    stats = {\n",
    "        'docs_total': 0,\n",
    "        'docs_sans_parti': 0,\n",
    "        'docs_sans_texte': 0,\n",
    "        'docs_texte_vide': 0,\n",
    "        'docs_valides': 0\n",
    "    }\n",
    "\n",
    "    for doc in racine.findall('doc'):\n",
    "        stats['docs_total'] += 1\n",
    "\n",
    "        # Extraction parti\n",
    "        elem_parti = doc.find('.//PARTI')\n",
    "        if elem_parti is None:\n",
    "            stats['docs_sans_parti'] += 1\n",
    "\n",
    "        parti = elem_parti.get('valeur', 'INCONNU') if elem_parti is not None else 'INCONNU'\n",
    "\n",
    "        # Extraction texte\n",
    "        elem_texte = doc.find('texte')\n",
    "        parties_texte = []\n",
    "\n",
    "        if elem_texte is not None:\n",
    "            for p in elem_texte.findall('p'):\n",
    "                if p.text:\n",
    "                    parties_texte.append(p.text.strip())\n",
    "        else:\n",
    "            stats['docs_sans_texte'] += 1\n",
    "\n",
    "        texte = ' '.join(parties_texte)\n",
    "\n",
    "        if not texte:\n",
    "            stats['docs_texte_vide'] += 1\n",
    "\n",
    "        # Ne garder que docs avec texte\n",
    "        if texte:\n",
    "            stats['docs_valides'] += 1\n",
    "            interventions.append({\n",
    "                'parti': parti,\n",
    "                'texte': texte\n",
    "            })\n",
    "\n",
    "    duree = time.time() - debut\n",
    "\n",
    "    if verbose:\n",
    "        print(f\"\\n[CHARGEMENT] {Path(chemin).name}\")\n",
    "        print(f\"  Durée : {duree:.2f}s\")\n",
    "        print(f\"  Documents totaux : {stats['docs_total']:,}\")\n",
    "        print(f\"  Documents chargés : {stats['docs_valides']:,} ({stats['docs_valides']/stats['docs_total']*100:.1f}%)\")\n",
    "\n",
    "        if stats['docs_sans_parti'] > 0:\n",
    "            print(f\"    Sans parti : {stats['docs_sans_parti']:,}\")\n",
    "        if stats['docs_sans_texte'] > 0:\n",
    "            print(f\"    Sans texte : {stats['docs_sans_texte']:,}\")\n",
    "        if stats['docs_texte_vide'] > 0:\n",
    "            print(f\"    Texte vide : {stats['docs_texte_vide']:,}\")\n",
    "\n",
    "    return interventions\n",
    "\n",
    "\n",
    "# Chargement\n",
    "print(\"=\"*80)\n",
    "print(\"CHARGEMENT DONNÉES\")\n",
    "print(\"=\"*80)\n",
    "\n",
    "interventions_train = charger_xml(chemin_train, verbose=True)\n",
    "interventions_test = charger_xml(chemin_test, verbose=True) if chemin_test else []\n",
    "\n",
    "print(f\"\\n[OK] Chargement terminé\")\n",
    "print(f\"  Train : {len(interventions_train):,} interventions\")\n",
    "print(f\"  Test  : {len(interventions_test):,} interventions\")"
